#include <iostream>

using namespace std;

const int MAXN = 101;

int main( void ){
    int n;
	while( cin >> n ){
		int square = 0;
		int len = 0;
		int a[MAXN][MAXN];
		for( int i = 0; i < n; ++i ){
			for( int j = 0; j < n; ++j ){
				cin >> a[i][j];
			}
		}
		for( int i = 0; i < n; ++i ){
			for( int j = 0; j < n; ++j ){
				if( a[i][j] <= 50 ){
					++square;
					if( i == 0 || j == 0 || i == n-1 || j == n-1 ){//ÅÐ¶ÏÊÇ·ñÎª±ß½ç
						++len;
					}else if( a[i+1][j] > 50 || a[i-1][j] > 50 || a[i][j+1] > 50 || a[i][j-1] > 50 ){
						++len;
					}
				}
			}
		}
		cout << square << " " << len << endl;
	}
    return 0;
}
